database demo
two tables
login and blog

create table login (
	id serial,
    loginid varchar(100),
    password varchar(100)
    );

insert into login values(1, 'alishadhawan@gmail.com', '12345678');


create table blog (
    blog_id serial,
    title varchar(100),
    blog varchar(200),
    user_id varchar(100)
    );